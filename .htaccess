# IOC Simple Routing - Static files handled by serve-static.php
RewriteEngine On

# Only route URLs that don't match actual files or directories
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_URI} !^/serve-static\.php
RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]
